        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(obj_Player.Holding == 0 || ((obj_Player.Holding == 1 &amp;&amp; Hold == obj_Player.Index)))
{
    //if clicking in the chart title
    if( ((mouse_x &gt;= Title_Left_Bound) &amp;&amp; (mouse_x &lt;= Title_Right_Bound)) &amp;&amp; ((mouse_y &gt;= Title_Upper_Bound) &amp;&amp; (mouse_y &lt;= Title_Lower_Bound))  )
    {
        if(Tracker == 0)
        {
            Tracker = 5;
        }
        else if(Tracker &gt; 0)
        {
            if(isWriting=false) 
            {
                with(obj_Chart)
                {
                    isWriting=false;
                }
                obj_ChatInput.isWriting = false;
            
                isWriting=true;
                keyboard_string="";
                
            }
            
        }
        
    }
    //if clicking in the Trade increment button
    if( ((mouse_x &gt;= Trade_Increment_Left_Bound) &amp;&amp; (mouse_x &lt;= Trade_Increment_Right_Bound)) &amp;&amp; ((mouse_y &gt;= Trade_Increment_Upper_Bound) &amp;&amp; (mouse_y &lt;= Trade_Increment_Lower_Bound))  )
    {
        Trade += 1;
        if(Trade == Limit)
        {
            Trade = 0;
        }
        scr_SendUpdateCatanChartScores();
    }
    //if clicking in the Trade decrement button
    else if( ((mouse_x &gt;= Trade_Decrement_Left_Bound) &amp;&amp; (mouse_x &lt;= Trade_Decrement_Right_Bound)) &amp;&amp; ((mouse_y &gt;= Trade_Decrement_Upper_Bound) &amp;&amp; (mouse_y &lt;= Trade_Decrement_Lower_Bound))  )
    {
        Trade -= 1;
        if(Trade &lt; 0)
        {
            Trade = (Limit-1);
        }
        scr_SendUpdateCatanChartScores();
    }
    //if clicking in the Politics increment button
    else if( ((mouse_x &gt;= Politics_Increment_Left_Bound) &amp;&amp; (mouse_x &lt;= Politics_Increment_Right_Bound)) &amp;&amp; ((mouse_y &gt;= Politics_Increment_Upper_Bound) &amp;&amp; (mouse_y &lt;= Politics_Increment_Lower_Bound))  )
    {
        Politics += 1;
        if(Politics == Limit)
        {
            Politics = 0;
        }
        scr_SendUpdateCatanChartScores();
    }
    //if clicking in the Politics decrement button
    else if( ((mouse_x &gt;= Politics_Decrement_Left_Bound) &amp;&amp; (mouse_x &lt;= Politics_Decrement_Right_Bound)) &amp;&amp; ((mouse_y &gt;= Politics_Decrement_Upper_Bound) &amp;&amp; (mouse_y &lt;= Politics_Decrement_Lower_Bound))  )
    {
        Politics -= 1;
        if(Politics &lt; 0)
        {
            Politics = (Limit-1);
        }
        scr_SendUpdateCatanChartScores();
    }
    //if clicking in the Building increment button
    else if( ((mouse_x &gt;= Building_Increment_Left_Bound) &amp;&amp; (mouse_x &lt;= Building_Increment_Right_Bound)) &amp;&amp; ((mouse_y &gt;= Building_Increment_Upper_Bound) &amp;&amp; (mouse_y &lt;= Building_Increment_Lower_Bound))  )
    {
        Building += 1;
        if(Building == Limit)
        {
            Building = 0;
        }
        scr_SendUpdateCatanChartScores();
    }
    //if clicking in the Building decrement button
    else if( ((mouse_x &gt;= Building_Decrement_Left_Bound) &amp;&amp; (mouse_x &lt;= Building_Decrement_Right_Bound)) &amp;&amp; ((mouse_y &gt;= Building_Decrement_Upper_Bound) &amp;&amp; (mouse_y &lt;= Building_Decrement_Lower_Bound))  )
    {
        Building -= 1;
        if(Building &lt; 0)
        {
            Building = (Limit-1);
        }
        scr_SendUpdateCatanChartScores();
    }
    //if clicking in the Victory increment button
    else if( ((mouse_x &gt;= Victory_Increment_Left_Bound) &amp;&amp; (mouse_x &lt;= Victory_Increment_Right_Bound)) &amp;&amp; ((mouse_y &gt;= Victory_Increment_Upper_Bound) &amp;&amp; (mouse_y &lt;= Victory_Increment_Lower_Bound))  )
    {
        Score += 1;
        scr_SendUpdateCatanChartScores();
    }
    //if clicking in the Victory decrement button
    else if( ((mouse_x &gt;= Victory_Decrement_Left_Bound) &amp;&amp; (mouse_x &lt;= Victory_Decrement_Right_Bound)) &amp;&amp; ((mouse_y &gt;= Victory_Decrement_Upper_Bound) &amp;&amp; (mouse_y &lt;= Victory_Decrement_Lower_Bound))  )
    {
        Score -= 1;
        scr_SendUpdateCatanChartScores();
    }
    //if clicking in the Gold increment button
    else if( ((mouse_x &gt;= Gold_Increment_Left_Bound) &amp;&amp; (mouse_x &lt;= Gold_Increment_Right_Bound)) &amp;&amp; ((mouse_y &gt;= Gold_Increment_Upper_Bound) &amp;&amp; (mouse_y &lt;= Gold_Increment_Lower_Bound))  )
    {
        Gold += 1;
        scr_SendUpdateCatanChartScores();
    }
    //if clicking in the Gold decrement button
    else if( ((mouse_x &gt;= Gold_Decrement_Left_Bound) &amp;&amp; (mouse_x &lt;= Gold_Decrement_Right_Bound)) &amp;&amp; ((mouse_y &gt;= Gold_Decrement_Upper_Bound) &amp;&amp; (mouse_y &lt;= Gold_Decrement_Lower_Bound))  )
    {
        Gold -= 1;
        scr_SendUpdateCatanChartScores();
    }
    else
    {
    
        if(Hold == (-1) || Hold == obj_Player.Index)
        {
            if(Selected == false)
            {
                Selected = true;
                obj_Player.Holding = 1;
                temp_x = mouse_x - x;
                temp_y = mouse_y - y;
                depth = obj_Controller.PickupHeight;
                scr_SendUpdateObjectDepth();
            }
            else if(Selected == true)
            {
            
                Selected = false;
                obj_Player.Holding = 0;
                temp_x = 0;
                temp_y = 0;
                scr_DetermineGameObjectHeight();

                if(position_meeting(x,y,obj_Table) == false)
                {
                    instance_destroy();
                }
            }
            
            if(Hold == (-1))
             {
                Hold = obj_Player.Index;
             }
             else if(Hold == obj_Player.Index)
             {
                Hold = (-1);
             }
             scr_SendUpdateObjectHold();
        }
    }
