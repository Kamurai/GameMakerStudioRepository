        <arguments>
          <argument>
            <kind>1</kind>
            <string>
//zombie chase code

//image_angle = direction;


if( distance_to_object(person) &lt; 48)
{
    //if there is a person to the right
    if ( (x) &lt; person.x )
    {
        //check for wall in between self and other
        //if( place_meeting(x+23+3, y, wall) == true)
        if( ( place_meeting((x+23)+10, (y)+11, wall) == true ) || (( place_meeting((x+23)+10, (y)+11, wall) == true )&amp;&amp; ( place_meeting((x)+11, (y)-5, wall) == true || place_meeting((x)+11, (y+23)+5, wall) == true  )))//(place_meeting((x+23)+5, (y), wall) == true) &amp;&amp; (place_meeting((x+23)+5, (y)+11, wall) == true)&amp;&amp; (place_meeting((x+23)+5, (y)+23, wall) == true)&amp;&amp; (place_meeting((x+23)+5, (y)+5, wall) == true) &amp;&amp; (place_meeting((x+23)+5, (y)+17, wall) == true) )
        {
        //ignore person
        }    
        //intense checking: right
        else if( (position_empty((x+23)+1, (y)) == true) &amp;&amp; (position_empty((x+23)+1, (y)+11) == true)&amp;&amp; (position_empty((x+23)+1, (y)+23) == true)&amp;&amp; (position_empty((x+23)+1, (y)+5) == true) &amp;&amp; (position_empty((x+23)+1, (y)+17) == true))
        {
        //move right
        hspeed = global.zspeed;
        }
        else if (distance_to_object(person) &lt; 12)
        {
        //move right
        hspeed = global.zspeed;
        }
        else if (distance_to_object(zombie) &lt; 12)
        {
        //move right
        hspeed = global.zspeed;
        }
        

    }
    //if there is a person to the left
    else if( (x) &gt; person.x)
    {
    
        //check for wall in between self and other
        //if( place_meeting(x-3, y, wall) == true)
        if( ( place_meeting((x)-10, (y)+11, wall) == true ) || (( place_meeting((x)-10, (y)+11, wall) == true )&amp;&amp; ( place_meeting((x)+11, (y)-5, wall) == true || place_meeting((x)+11, (y+23)+5, wall) == true  )))//(place_meeting((x)-5, (y), wall) == true) &amp;&amp; (place_meeting((x)-5, (y)+11, wall) == true)&amp;&amp; (place_meeting((x)-5, (y)+23, wall) == true)&amp;&amp; (place_meeting((x)-5, (y)+5, wall) == true) &amp;&amp; (place_meeting((x)-5, (y)+17, wall) == true) )
        {
            //ignore person
        }    
        //intense checking: left
        else if( (position_empty((x)-1, (y)) == true) &amp;&amp; (position_empty((x)-1, (y)+11) == true)&amp;&amp; (position_empty((x)-1, (y)+23) == true)&amp;&amp; (position_empty((x)-1, (y)+5) == true) &amp;&amp; (position_empty((x)-1, (y)+17) == true) )
        {

        //move left
        hspeed = -global.zspeed;
        }
        else if ( distance_to_object(person) &lt; 12)
        {
        //move left
        hspeed = -global.zspeed;
        
        }
        else if ( distance_to_object(zombie) &lt; 12)
        {
        //move left
        hspeed = -global.zspeed;
        
        }

        
    }  
    
       
    
    //if there is a person above the zombie
    if( (y) &gt; person.y)
    {
        //check for wall in between self and other
        //if( place_meeting(x, y-3, wall) == true)
        if( (place_meeting((x)+11, (y)-5, wall) == true) || (( place_meeting((x)+11, (y)-5, wall) == true )&amp;&amp; ( place_meeting((x+23)+10, (y)+11, wall) == true || place_meeting((x)-10, (y)+11, wall) == true  )))//(place_meeting((x), (y)-3, wall) == true) &amp;&amp; (place_meeting((x)+11, (y)-3, wall) == true)&amp;&amp; (place_meeting((x)+23, (y)-3, wall) == true)&amp;&amp; (place_meeting((x)+5, (y)-3, wall) == true) &amp;&amp; (place_meeting((x)+17, (y)-3, wall) == true) )
        {
        //ignore person
        }    
        //intense checking: up
        else if( (position_empty((x), (y)-1) == true) &amp;&amp; (position_empty((x)+11, (y)-1) == true)&amp;&amp; (position_empty((x)+23, (y)-1) == true)&amp;&amp; (position_empty((x)+5, (y)-1) == true) &amp;&amp; (position_empty((x)+17, (y)-1) == true) )
        {

        //move up
        vspeed = -global.zspeed;
        }
        else if( distance_to_object(person) &lt; 12)
        {
        //move right
        vspeed = -global.zspeed;
        
        }
        else if( distance_to_object(zombie) &lt; 12)
        {
        //move right
        vspeed = -global.zspeed;
        
        }

        
        
    }
    //if there is a person below the zombie
    else if( (y) &lt; person.y)
    {
        //check for wall in between self and other
        //if( place_meeting(x, y+23+3, wall) == true)
        if( (place_meeting((x)+11, (y+23)+5, wall) == true) || (( place_meeting((x)+11, (y+23)+5, wall) == true )&amp;&amp; ( place_meeting((x+23)+10, (y)+11, wall) == true || place_meeting((x)-10, (y)+11, wall) == true  )))//(place_meeting((x), (y+23)+3, wall) == true) &amp;&amp; (place_meeting((x)+11, (y+23)+3, wall) == true)&amp;&amp; (place_meeting((x)+23, (y+23)+3, wall) == true)&amp;&amp; (place_meeting((x)+5, (y+23)+3, wall) == true) &amp;&amp; (place_meeting((x)+17, (y+23)+3, wall) == true) )
        {
        //ignore person
        }    
        //intense checking: down
        else if( (position_empty((x), (y+23)+1) == true) &amp;&amp; (position_empty((x)+11, (y+23)+1) == true)&amp;&amp; (position_empty((x)+23, (y+23)+1) == true)&amp;&amp; (position_empty((x)+5, (y+23)+1) == true) &amp;&amp; (position_empty((x)+17, (y+23)+1) == true) )
        {

        //move down
        vspeed = global.zspeed;
        }
        else if (distance_to_object(person) &lt; 12)
        {
        //move right
        vspeed = global.zspeed;
        
        }
        else if (distance_to_object(zombie) &lt; 12)
        {
        //move right
        vspeed = global.zspeed;
        
        }

        
        
    }
   
    
}
