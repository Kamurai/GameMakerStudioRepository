    case 0:
        //(old player) New player create
        var playerid = buffer_read(nsv_message, buffer_u32), ins;
        if ns_get_my_player_id(global.handle)!=playerid
        {
            ins = instance_create(10, 10, obj_player)
            ins.control = playerid
            ins.color = 1
            /**/buffer_seek(nsv_send, buffer_seek_start, 0)
            ns_message_send(global.handle, to_my_space, playerid, 1)
        }
        break
    case 1:
        //(new player) old player create
        var ins;
        ins = instance_create(10, 10, obj_player)
        ins.control = ns_get_message_sent_player()
        ins.color = 1
        break
    case 2:
        //player instance x, y
        with(obj_player)
        {
            if control=ns_get_message_sent_player()
            {
                x = buffer_read(nsv_message, buffer_u16)
                y = buffer_read(nsv_message, buffer_u16)
                break
            }
        }
        break
    case 3:
        //player instance destroy
        var playerid = buffer_read(nsv_message, buffer_u32);
        with(obj_player)
        {
            if control=playerid
            {
                instance_destroy()
            }
        }
        break
}
</string>
